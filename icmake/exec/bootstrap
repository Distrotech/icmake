echo Creating icm-exec

gcc -O2 -g -Wall -DHAVE_GLOB -c *.c 

mkdir -p tmp/bin

NR=0
for x in aux var virtual int list string stack opcodefun builtin
do
    cd $x
    gcc -O2 -g -Wall -DHAVE_GLOB -c *.c || exit 1
#    for o in *.o
#    do
#        mv $o ${NR}$o
#    done
    cd ..
done

gcc -o tmp/bin/icm-exec *.o */*.o ../rss/libicrss.a -s
# gcc -o ../bin/icm-exec *.o */*.o ../rss/libicrss.a

rm *.o */*.o
